{
  "cli": {
    "version": ">= 16.7.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "env": {
        "SENTRY_AUTH_TOKEN": "SENTRY_AUTH_TOKEN",
        "SENTRY_ORG": "rebelpug",
        "SENTRY_PROJECT": "piggus"
      }
    },
    "preview": {
      "developmentClient": false,
      "autoIncrement": true,
      "ios": {
        "simulator": true
      },
      "env": {
        "SENTRY_AUTH_TOKEN": "SENTRY_AUTH_TOKEN",
        "SENTRY_ORG": "rebelpug",
        "SENTRY_PROJECT": "piggus"
      }
    },
    "production": {
      "developmentClient": false,
      "autoIncrement": true,
      "env": {
        "SENTRY_AUTH_TOKEN": "SENTRY_AUTH_TOKEN",
        "SENTRY_ORG": "rebelpug",
        "SENTRY_PROJECT": "piggus"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
      },
      "ios": {
      }
    }
  }
}
